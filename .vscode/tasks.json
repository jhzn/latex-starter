{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Start latex docker daemon",
			"type": "shell",
			"command": "latexdockerdaemon.sh",
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": true,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": false
			}
		},
		{
			"label": "Stop latex docker daemon",
			"type": "shell",
			"command": "docker",
			"args": [
				"stop",
				"latex_daemon"
			]
		}
	]
}
